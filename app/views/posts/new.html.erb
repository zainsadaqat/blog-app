<h2>Create a new Post</h2>
<%= form_with(model: post, url: posts_path, method: :post, local: true) do |f| %>
<div>
  <%= f.label :title %><br />
  <%= f.text_field :title %>
</div>
<div>
  <%= f.label :text %><br />
  <%= f.text_field :text %>
</div>
<div>
  <%= f.submit "Create a new Post" %>
</div>
<% end %>

<%= link_to "Back", posts_path%>
